<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
    <CheckBoxPreference
        android:key="pref_repeat"
        android:title="@string/pref_repeat"
        android:summary="%s"
        android:defaultValue="true" />
    <ListPreference
        android:key="pref_compression"
        android:title="@string/pref_compression"
        android:summary="%s, Compress Quality 0-100"
        android:dialogTitle="@string/pref_compression"
        android:entries="@array/pref_compressions_alias"
        android:entryValues="@array/pref_compressions"
        android:defaultValue="20"
        />
</PreferenceScreen>